<Application x:Class="TemporaTasks.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TemporaTasks"
             StartupUri="MainWindow.xaml">
    
    <Application.Resources>

        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resources/Dictionaries/Colors.xaml"/>
                <ResourceDictionary Source="/Resources/Dictionaries/TextBoxes.xaml"/>
                <ResourceDictionary Source="/Resources/Dictionaries/Icons.xaml"/>
                <ResourceDictionary Source="/Resources/Dictionaries/ToolTips.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
        
    </Application.Resources>
    
</Application>
